<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RuleDef>
    <defName>InsectoidBase</defName>
    <symbol>insectoidBase</symbol>
    <resolvers>
      <li Class="VFEI.SymbolResolver_Ibase" />
    </resolvers>
  </RuleDef>

  <RuleDef>
    <defName>InsectoidBaseLightning</defName>
    <symbol>insectoidBaseLightning</symbol>
    <resolvers>
      <li Class="VFEI.SymbolResolver_OutdoorLightingIbase" />
    </resolvers>
  </RuleDef>

  <RuleDef>
    <defName>InsectoidBaseRCorpse</defName>
    <symbol>insectoidBaseRCorpse</symbol>
    <resolvers>
      <li Class="VFEI.SymbolResolver_RandomCorpse" />
    </resolvers>
  </RuleDef>

  <RuleDef>
    <defName>InsectoidBaseRDamage</defName>
    <symbol>insectoidBaseRDamage</symbol>
    <resolvers>
      <li Class="VFEI.SymbolResolver_RandomDamage" />
    </resolvers>
  </RuleDef>

  <RuleDef>
    <defName>InsectoidRandHives</defName>
    <symbol>insectoidRandHives</symbol>
    <resolvers>
      <li Class="VFEI.SymbolResolver_RandomHives" />
    </resolvers>
  </RuleDef>

  <!-- ============ No PAWNS ============ -->
  <RuleDef>
		<defName>VFEI_SettlementNoPawns</defName>
		<symbol>settlementNoPawns</symbol>
		<resolvers>
			<li Class="VFEI.SymbolResolver_SettlementNoPawns">
				<minRectSize>(12, 12)</minRectSize>
			</li>
		</resolvers>
	</RuleDef>

	<RuleDef>
		<defName>VFEI_EdgeMannedMortarNoPawns</defName>
		<symbol>edgeMannedMortarNoPawns</symbol>
		<resolvers>
			<li Class="VFEI.SymbolResolver_EdgeMannedMortarNoPawns">
				<minRectSize>(3, 3)</minRectSize>
			</li>
		</resolvers>
	</RuleDef>

	<RuleDef>
		<defName>VFEI_MannedMortarNoPawns</defName>
		<symbol>mannedMortarNoPawns</symbol>
		<resolvers>
			<li Class="VFEI.SymbolResolver_MannedMortarNoPawns">
				<minRectSize>(3, 3)</minRectSize>
			</li>
		</resolvers>
	</RuleDef>

	<RuleDef>
		<defName>VFEI_EdgeDefenseNoPawns</defName>
		<symbol>edgeDefenseNoPawns</symbol>
		<resolvers>
			<li Class="VFEI.SymbolResolver_EdgeDefenseNoPawn">
				<minRectSize>(2, 2)</minRectSize>
			</li>
		</resolvers>
